{
    "name": "Tvheadend",
    "id": "tvheadend",
    "packaging_format": 1,
    "description": {
        "en": "TV streaming server and recorder",
        "fr": "Serveur de streaming et d'enregistrement TV"
    },
    "version": "4.3.2009~ynh10",
    "url": "https://tvheadend.org",
    "upstream": {
        "license": "GPL-3.0",
        "website": "https://tvheadend.org",
        "userdoc": "https://tvheadend.org/projects/tvheadend/wiki",
        "code": "https://github.com/tvheadend/tvheadend"
    },
    "license": "GPL-3.0",
    "maintainer": {
        "name": "Sylvain Cecchetto",
        "email": "cecchetto.sylvain@me.com"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/tvh",
                "default": "/tvh"
            },
            {
                "name": "superuser",
                "type": "string",
                "ask": {
                    "en": "Choose the superuser name in order to login on Tvheadend",
                    "fr": "Choisissez le nom du superutilisateur pour la connexion Ã  Tvheadend"
                },
                "example": "johndoe"
            },
            {
                "name": "password",
                "type": "password"
            }
        ]
    }
}
